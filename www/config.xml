<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jaytorian.voyager" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">

    <name>Voyager2</name>
    <description>
        A basic employee directory - experimental application
    </description>
    <author email="jaytorian@gmail.com" href="http://www.jaytorian.com">
        Jay Torian
    </author>
    <content src="index.html" />
    <access origin="*" />
    <!-- set to prevent iOS from scrolling header -->
    <preference name="DisallowOverscroll" value="true" />
    <!-- set phonegap preferences -->    
   <preference name="phonegap-version" value="3.1.0" />
    <gap:plugin name="org.apache.cordova.camera" />
    <gap:plugin name="org.apache.cordova.device" />
    <gap:plugin name="org.apache.cordova.contacts" />               
	 <gap:plugin name="org.apache.cordova.dialogs" />	 
	 <gap:plugin name="org.apache.cordova.console" />
	 	 	 	 	 
</widget>
